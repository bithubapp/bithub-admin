<div class="form-group">
	<label>User</label>
	<div class="suggest-wrapper">
		<select {{instagramSuggest}} can-value="map.config.id" placeholder="{{#if map.config.id}}Current User: {{map.config.display_name}}{{else}}Enter Instagram User{{/if}}">
		</select>
		<i class="fa {{#if isLoading}}fa-circle-o-notch fa-spin{{else}}fa-user{{/if}}"></i>
	</div>
	
	{{{configErrors errors 'id'}}}
</div>