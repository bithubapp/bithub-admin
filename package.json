{
  "name": "bithub-admin",
  "version": "0.1.0",
  "description": "Bithub Administration App",
  "homepage": "bithub.com/admin",
  "author": "Bitovi",
  "scripts": {
    "test": "testee src/test.html --browsers firefox --reporter Spec",
    "lint": "xo",
    "start": "done-serve --proxy https://develop.bithub.com --port 8080",
    "develop": "done-serve --proxy https://develop.bithub.com/ --develop --port 8080",
    "document": "documentjs",
    "build": "node bin/build",
    "preflight": "npm run lint && npm test && npm run build",
    "push-to-git": "git push origin --tags",
    "release:patch": "npm run preflight && npm version patch && npm run push-to-git",
    "release:minor": "npm run preflight && npm version minor && npm run push-to-git",
    "release:major": "npm run preflight && npm version major && npm run push-to-git",
    "test:debug": "DEBUG=testee:* testee -b chrome src/test.html --reporter Spec"
  },
  "main": "bithub-social.js",
  "files": [
    "src"
  ],
  "keywords": [
    "bithub"
  ],
  "system": {
    "main": "bithub-admin/index.stache!done-autorender",
    "directories": {
      "lib": "src"
    },
    "configDependencies": [
      "live-reload",
      "node_modules/can-zone/register"
    ],
    "npmAlgorithm": "flat",
    "map": {
      "chai": "chai/chai"
    }
  },
  "license": "ISC",
  "dependencies": {
    "can-component": "^3.0.0-pre.8",
    "can-connect": "^0.6.0-pre.4",
    "can-define": "^0.7.4",
    "can-route": "^3.0.0-pre.5",
    "can-route-pushstate": "^3.0.0-pre.3",
    "can-stache": "^3.0.0-pre.6",
    "can-validate": "^0.9.2",
    "can-view-autorender": "^3.0.0-pre.3",
    "can-view-model": "^3.0.0-pre.4",
    "can-zone": "^0.5.5",
    "done-autorender": "^0.9.0-pre.0",
    "done-component": "^0.6.0-pre.1",
    "done-css": "~2.1.0-pre.0",
    "done-serve": "^0.3.0-pre.0",
    "generator-donejs": "^0.10.0-pre.0",
    "jquery": "~2.2.1",
    "selectize": "^0.12.3",
    "steal": "^0.16.0",
    "steal-stache": "^3.0.0-pre.2",
    "validate.js": "^0.10.0"
  },
  "devDependencies": {
    "can-fixture": "^0.1.2",
    "chai": "^3.5.0",
    "documentjs": "^0.4.2",
    "donejs-cli": "^0.9.4",
    "donejs-deploy": "^0.4.0",
    "funcunit": "~3.0.0",
    "steal-mocha": "0.0.3",
    "steal-qunit": "^0.1.1",
    "steal-tools": "^0.16.0",
    "testee": "^0.2.4",
    "xo": "^0.16.0"
  },
  "xo": {
    "esnext": true,
    "envs": [
      "node",
      "jasmine"
    ],
    "rules": {
      "import/no-extraneous-dependencies": 0,
      "prefer-arrow-callback": 0
    },
    "globals": [
      "steal"
    ],
    "ignore": [
      "node_modules/**",
      "dist/**",
      "docs/**"
    ],
    "overrides": [
      {
        "files": "bin/**",
        "esnext": false
      }
    ]
  }
}
